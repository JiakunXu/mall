<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
	
	<bean id="ipService" class="com.kintiger.mall.ip.service.impl.IPServiceImpl">
		<property name="memcachedCacheService" ref="memcachedCacheService" />
		<property name="ipDao" ref="ipDao" />
		<property name="url">
			<value>${taobao.ip.url}</value>
		</property>
	</bean>
	
	<bean id="ipDao" class="com.kintiger.mall.ip.dao.impl.IPDaoImpl" parent="baseDao" />
	
</beans>